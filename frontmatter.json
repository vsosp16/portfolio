{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "toc",
          "name": "toc",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "portfolio",
      "path": "[[workspace]]"
    },
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "portfolio",
      "path": "[[workspace]]/content/portfolio"
    },
    {
      "title": "about",
      "path": "[[workspace]]/content/about"
    },
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "WordPress",
      "path": "[[workspace]]/content/portfolio/WordPress"
    },
    {
      "title": "about",
      "path": "[[workspace]]/content/about"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    }
  ],
  "frontMatter.content.snippets": {},
  "frontMatter.dashboard.openOnStart": true
}